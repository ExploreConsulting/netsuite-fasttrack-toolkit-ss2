{
  "automock": false,
  "clearMocks": true,
  "moduleDirectories": [
    "node_modules",
    "."
  ],
  "moduleNameMapper": {
    "NFT/(.*)": "../$1",
    "N/(.*)": "<rootDir>/../__mocks__/N/$1",
    "./moment": "node_modules/moment",
    "./aurelia-logging": "node_modules/aurelia-logging",
    "./lodash": "node_modules/lodash"
  },
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "warnOnly": false
      }
    }
  },
  "preset": "ts-jest",
  "testMatch": null
}

