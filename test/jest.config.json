{
  "automock": false,
  "clearMocks":true,
  "moduleDirectories" :["node_modules","."],
  "moduleNameMapper":{
    "NFT/(.*)": "../$1",
    "N/(.*)": "<rootDir>/../__mocks__/N/$1"
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ]
}


