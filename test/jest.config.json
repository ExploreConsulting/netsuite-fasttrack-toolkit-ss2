{
  "automock": false,
  "clearMocks": true,
  "moduleDirectories": [
    "node_modules"
  ],
  "transform": {
  },
  "moduleNameMapper": {
    "NFT/(.*)": "../$1",
    "N/(.*)": "<rootDir>/../__mocks__/N/$1",
    "./moment": "moment",
    "./aurelia-logging": "aurelia-logging",
    "./lodash": "lodash"
  },
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "testPathIgnorePatterns": ["/integration/"],
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "warnOnly": false
      }
    }
  },
  "preset": "ts-jest",
  "testMatch": null
}

